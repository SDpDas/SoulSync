# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=https://soulsyncs-backend.onrender.com/
VITE_APP_NAME=SoulSync
VITE_APP_VERSION=1.0.0

# Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Feature Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_REALTIME_STATS=true
VITE_ENABLE_GEMINI_MATCHING=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_ANIMATIONS=true

# Storage Configuration
VITE_STORAGE_TYPE=localStorage
VITE_SESSION_TIMEOUT=3600000

# Dating App Specific
VITE_MAX_MATCHES_PER_DAY=50
VITE_MAX_MESSAGES_PER_CONVERSATION=1000
VITE_PROFILE_PHOTO_LIMIT=6